<% @user.errors.full_messages.each do |message| %>
    <div class="form-error">
      <%= message %>
    </div>
  <% end %>
  
<p>ユーザー編集</p>
<%= form_with model:@user,local:true do |form| %>
<p>ユーザー名</p>
<%= form.text_field :name %>
<p>画像</p>
<%= form.file_field :avator %>
<%= form.submit %>
<%end%>

